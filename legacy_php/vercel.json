{
    "version": 2,
    "builds": [
        {
            "src": "*.php",
            "use": "vercel-php@0.6.0"
        },
        {
            "src": "assets/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(assets/.*)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}